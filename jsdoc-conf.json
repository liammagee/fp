{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true,
        "default": {
            "layoutFile": "docs/templates/jsdoc-layout.tmpl",
            "outputSourceFiles": true
        }
    }
}
